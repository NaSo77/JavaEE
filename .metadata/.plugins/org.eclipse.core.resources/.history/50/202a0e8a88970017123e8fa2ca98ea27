package files;

import java.io.File;
import java.io.IOException;

public class Files {
    public static void main(String[] args) {
	File file = new File("folder1" + File.separator + "folder2", "file1.txt");
	try {
	    file.createNewFile();
	} catch (IOException e) {
	    // TODO Auto-generated catch block
	    e.printStackTrace();
	}
	
    }
}
